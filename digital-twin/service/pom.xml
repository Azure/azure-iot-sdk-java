<project>
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.microsoft.azure.sdk.iot</groupId>
        <artifactId>iot-sdk-java-digital-twin</artifactId>
        <version>1.0.0</version>
    </parent>
    <name>Azure IoT Sdk Java Digital Twin Service</name>
    <description>The Microsoft Azure IoT Digital Twin Service SDK for Java</description>
    <artifactId>iot-sdk-java-digital-twin-service</artifactId>
    <version>${project-service-version}</version>
    <packaging>jar</packaging>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
                <version>${maven-javadoc-plugin-version}</version>
                <configuration>
                    <includeDependencySources>true</includeDependencySources>
                    <show>protected</show>
                    <sourcepath>${project.build.directory}/delombok</sourcepath>
                    <excludePackageNames>com.microsoft.azure.sdk.iot.digitaltwin.service.util</excludePackageNames>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
        <!-- https://mvnrepository.com/artifact/com.microsoft.rest/client-runtime -->
        <dependency>
            <groupId>com.microsoft.rest</groupId>
            <artifactId>client-runtime</artifactId>
            <version>${ms-rest-client-version}</version>
        </dependency>
    </dependencies>
</project>